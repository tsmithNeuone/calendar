<section id="tables">
  <div class="page-header">
    <h1><%= @event.title %></h1>
  </div>
  
  <table class="table table-bordered table-striped table-hover">
    <thead>
      <tr>
        <th>Starts at: </th>
        <th>Ends at:</th>
        <th>All day:</th>
        <th>Description:</th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td><%= @event.starts_at %></td>
        <td><%= @event.ends_at %></td>
        <td> <%= @event.all_day %></td>
        <td><%= @event.description %></td>
      </tr>
     
    </tbody>
  </table>
</section>
<td><a class="btn btn-primary" <%= link_to 'New Event', new_event_path %></a></td>
<td><a class="btn btn-primary" <%= link_to 'Edit', edit_event_path(@event) %></a></td>
<td><a class="btn btn-primary" <%= link_to 'All Events', events_path %></a></td>
<br>
<br>
<td><a class="btn btn-primary" <%= link_to 'Back to Calendar', calendar_index_path %></a></td>

